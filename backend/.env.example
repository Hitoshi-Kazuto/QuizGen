# Gemini API Key - Get from https://aistudio.google.com/app/apikey
GEMINI_API_KEY=your_gemini_api_key_here

# MongoDB Connection String - Format: mongodb+srv://<username>:<password>@<cluster>.mongodb.net/?retryWrites=true&w=majority
MONGO_URL=mongodb+srv://username:password@clusterxxx.mongodb.net/?retryWrites=true&w=majority

# Database Name
DB_NAME=your_database_name

# Secret Key for JWT Authentication (should be a strong random string in production)
SECRET_KEY=your-super-secret-key-for-jwt-tokens-change-this-in-production

